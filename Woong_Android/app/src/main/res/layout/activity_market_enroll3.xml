<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    tools:context=".join.marketer.market.Enroll3Activity">
    <RelativeLayout
        android:layout_width="279dp"
        android:layout_height="434dp"
        android:layout_centerInParent="true"
        android:background="@drawable/white_round_rec">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_centerInParent="true">
            <ImageView
                android:layout_gravity="center"
                android:layout_width="160dp"
                android:layout_height="124dp"
                android:src="@drawable/seller_enter_ok_woong"/>
            <TextView
                android:fontFamily="@font/nanum_bold"
                android:textSize="15dp"
                android:textColor="#707070"
                android:layout_gravity="center"
                android:layout_marginTop="50dp"
                android:layout_marginBottom="43dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="등록이 완료되었습니다.\n결과는 1주일 후 e-mail로 전송됩니다."
                android:textAlignment="center"/>
            <Button
                android:id="@+id/btn_ok_enroll3"
                android:layout_width="188dp"
                android:layout_height="40dp"
                android:fontFamily="@font/nanum_extrabold"
                android:textSize="15dp"
                android:text="확인"
                android:textColor="@android:color/white"
                android:background="@drawable/green_round_button"
                android:layout_gravity="center_horizontal"/>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>